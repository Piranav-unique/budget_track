{
    "version": 2,
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.ts"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "framework": "vite",
    "installCommand": "npm install",
    "buildCommand": "npm run build:client",
    "outputDirectory": "dist/spa"
}